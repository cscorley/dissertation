    doi = {10.1109/ICPC.2012.6240485},
    institution = {Dept. of Comput. Sci., Univ. of Alabama, Tuscaloosa, AL, USA},
    isbn = {978-1-4673-1213-4},
    issn = {1063-6897},
    keywords = {expertise, ownership, q, topics},
    month = jun,
    pages = {173--182},
